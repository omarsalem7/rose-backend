{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "contact"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "requestStatus": {
      "type": "enumeration",
      "default": "new",
      "enum": [
        "new",
        "inprogress",
        "done",
        "fake"
      ]
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "message": {
      "type": "text"
    }
  }
}
