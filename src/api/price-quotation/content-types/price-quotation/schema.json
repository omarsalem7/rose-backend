{
  "kind": "collectionType",
  "collectionName": "price_quotations",
  "info": {
    "singularName": "price-quotation",
    "pluralName": "price-quotations",
    "displayName": "price-quotation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "companyName": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "specialRequests": {
      "type": "text"
    },
    "requestStatus": {
      "type": "enumeration",
      "private": true,
      "default": "new",
      "enum": [
        "new",
        "inprogress",
        "done",
        "fake"
      ]
    }
  }
}
