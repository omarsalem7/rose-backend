{
  "kind": "collectionType",
  "collectionName": "request_samples",
  "info": {
    "singularName": "request-sample",
    "pluralName": "request-samples",
    "displayName": "request-sample"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "requestStatus": {
      "type": "enumeration",
      "private": true,
      "default": "new",
      "enum": ["new", "inprogress", "done", "fake"]
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "companyName": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "specialRequests": {
      "type": "text"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    }
  }
}
