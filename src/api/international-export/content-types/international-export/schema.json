{
  "kind": "collectionType",
  "collectionName": "international_exports",
  "info": {
    "singularName": "international-export",
    "pluralName": "international-exports",
    "displayName": "international-export"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "requestStatus": {
      "type": "enumeration",
      "private": true,
      "default": "new",
      "enum": ["new", "inprogress", "done", "fake"]
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "companyName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "activity": {
      "type": "string"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "specialRequests": {
      "type": "text"
    },
    "chooseAllProducts": {
      "type": "boolean",
      "default": false
    }
  }
}
