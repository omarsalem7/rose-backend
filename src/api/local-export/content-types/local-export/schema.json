{
  "kind": "collectionType",
  "collectionName": "local_exports",
  "info": {
    "singularName": "local-export",
    "pluralName": "local-exports",
    "displayName": "local-export"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "requestStatus": {
      "type": "enumeration",
      "private": true,
      "default": "new",
      "enum": [
        "new",
        "inprogress",
        "done",
        "fake"
      ]
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "companyName": {
      "type": "string",
      "required": false
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "specialRequests": {
      "type": "text"
    },
    "chooseAllProducts": {
      "type": "boolean",
      "default": false
    }
  }
}
